@using ProductCategory.Models
@model AllProducts

@await Html.PartialAsync("_Navbar")

<div class="row text-center">
    <div class="col-md-6">
        <h2>New Product</h2>
        @await Html.PartialAsync("~/Views/Products/_NewProductForm.cshtml")
    </div>
    <div class="col-md-6">
        <h2>All Products</h2>
        @foreach (var product in Model.Products)
        {
            @await Html.PartialAsync("~/Views/Products/_ProductView.cshtml", product)
        }
    </div>
</div>
